<view class="container"><view class="tui-order-header"><view class="tui-text">订单编号：<text class="tui-bold">{{express.eid}}</text></view><view class="tui-text">{{"快递公司："+express.company}}</view><view class="tui-text">预计送达：<text class="tui-bold">{{express.expReach}}</text></view></view><view class="tui-order-tracking"><tui-time-axis vue-id="ca6b018e-1" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{$root.l0}}" wx:for-item="item" wx:for-index="index"><tui-timeaxis-item vue-id="{{('ca6b018e-2-'+index)+','+('ca6b018e-1')}}" backgroundColor="transparent" bind:__l="__l" vue-slots="{{['node','content']}}"><view slot="node"><block wx:if="{{index===0}}"><view class="tui-node" style="{{'background-color:'+(backgroundColor)+';'}}"><tui-icon vue-id="{{('ca6b018e-3-'+index)+','+('ca6b018e-2-'+index)}}" name="{{item.m0}}" color="#fff" size="{{14}}" bold="{{true}}" bind:__l="__l"></tui-icon></view></block><block wx:else><view class="tui-node"><tui-icon vue-id="{{('ca6b018e-4-'+index)+','+('ca6b018e-2-'+index)}}" name="{{item.m1}}" color="#fff" size="{{13}}" bind:__l="__l"></tui-icon></view></block></view><view slot="content"><block wx:if="{{item.$orig.status=='运输中'}}"><view><view class="tui-order-title">{{item.$orig.status}}</view><block wx:for="{{item.$orig.detail}}" wx:for-item="citem" wx:for-index="cindex"><view><view class="tui-order-desc">{{citem.subDetail}}</view><view class="tui-order-time tui-gray" style="margin-bottom:20rpx;">{{citem.subtime}}</view></view></block></view></block><block wx:else><view><view class="tui-order-title">{{item.$orig.status}}</view><view class="tui-order-desc">{{item.$orig.detail}}</view><view class="tui-order-time tui-gray">{{item.$orig.time}}</view></view></block></view></tui-timeaxis-item></block></tui-time-axis></view></view>